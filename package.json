{
  "name": "portfolio-api",
  "version": "1.0.0",
  "description": "API for Shingu Akira Portfolio",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "npx tsx src/index.ts",
    "dev:watch": "npx tsx watch src/index.ts",
    "test": "vitest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint:fix": "npm run format && npm run lint -- --fix"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@azure/functions": "^4.7.3-beta.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "azure-function-express": "^2.0.0",
    "eslint": "^8.57.1",
    "prettier": "^3.2.5",
    "serverless-http": "^3.2.0",
    "serverless-offline": "^14.4.0",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.0.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
